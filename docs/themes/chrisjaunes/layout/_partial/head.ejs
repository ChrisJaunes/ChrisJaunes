<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	<% if(config.description) { %><meta name="description" content="<%= config.description %>"><% } %>
	<% if(config.keywords) { %><meta name="keywords" content="<%= config.keywords %>"><% } %>
	<%
		var title = [];
		if (page.current > 1) title.push(__('page', page.current));
		if (page.title) title.push(page.title);
		if (page.category) title.push(page.category);
		if (page.tag) title.push(page.tag);
		if (page.archive) {
			if (page.year) title.push(__('archive', page.year + (page.month ? '/' + page.month : '')));
			else title.push(__('archive'));
		}
		title.push(config.title);
	%><title><%= title.join(' - ') %></title>
	<% if( theme.rss) { %><link rel="alternate" href="<%- theme.rss %>" type="application/atom+xml"/><% } %>
	<% if( theme.favicon) { %><link rel="shortcut icon" href="<%- url_for(theme.favicon) %>"/><% } %>
	<%- css('css/style') %>
	<%- css('https://cdn.staticfile.org/font-awesome/4.7.0/css/font-awesome.css') %>
</head>